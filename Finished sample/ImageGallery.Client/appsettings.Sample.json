{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ImageGalleryAPIRoot": "https://localhost:7075/",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://*:7184",
        "Certificate": {
          "KeyPath": "/etc/letsencrypt/live/yourdomain.com/privkey.pem",
          "Path": "/etc/letsencrypt/live/yourdomain.com/fullchain.pem"
        }
      }
    }
  }
}
